{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:init": "npm install prettier lint-staged husky -g",
    "lint:fix": "npm run lint:prettier_fix && npm run lint:es_fix",
    "lint:es": "eslint src/ --ext .js,.jsx,.vue",
    "lint:es_fix": "eslint --fix src/ --ext .js,.jsx,.vue",
    "lint:prettier": "prettier --check 'src/**/*.{js,jsx,vue,css,less,scss,sass}' --no-error-on-unmatched-pattern",
    "lint:prettier_fix": "prettier --write 'src/**/*.{js,jsx,vue,css,less,scss,sass}' --no-error-on-unmatched-pattern"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@kangc/v-md-editor": "^1.7.7",
    "ant-design-vue": "^1.7.8",
    "axios": "^0.21.4",
    "codemirror": "^5.63.0",
    "compression-webpack-plugin": "^5.0.2",
    "core-js": "^3.6.5",
    "echarts": "^5.0.1",
    "echarts-wordcloud": "^2.0.0",
    "hljs": "^6.2.3",
    "mixins": "0.0.1",
    "vcolorpicker": "^1.1.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^7.0.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
